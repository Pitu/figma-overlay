		"eslint": "^8.34.0",
		"eslint-config-neon": "^0.1.40",
		"eslint-plugin-vue": "^9.9.0",
		"npm-run-all": "^4.1.5",
		"prettier": "^2.8.4",
		"typescript": "~4.8.4",
		"vite": "^4.1.4",
